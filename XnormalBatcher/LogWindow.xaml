<Window x:Class="XnormalBatcher.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XnormalBatcher"
        xmlns:vm="clr-namespace:XnormalBatcher.ViewModels"
        mc:Ignorable="d"
        Title="LogWindow" Height="450" Width="800">
    <Grid>
        <ListBox ItemsSource="{Binding Source={x:Static vm:BatchViewModel.Instance}, Path=LogEntries}" />
    </Grid>
</Window>
